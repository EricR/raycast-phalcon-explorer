{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "phalcon-explorer",
  "title": "Phalcon Explorer",
  "description": "Integrates explorer.phalcon.xyz into Raycast.",
  "icon": "command-icon.png",
  "author": "EricR",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search",
      "subtitle": "Phalcon Explorer",
      "description": "Search for blockchain transactions using Phalcon Explorer.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.4.8",
    "axios": "^1.3.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
